{
  "version": 2,
  "routes": [
    {
      "src": "/(pl|en)",
      "dest": "/index.$1.server.html"
    },
    {
      "src": "/(pl|en)/(.*)",
      "dest": "/$1/$2"
    },
    {
      "src": "/",
      "dest": "/index.pl.server.html"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/resume-angular-2024/**/*",
      "memory": 512,
      "maxDuration": 10
    }
  }
}
